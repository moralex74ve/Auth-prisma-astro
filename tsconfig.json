{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "src", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@controllers/*": ["src/controllers/*"],
      "@data/*": ["src/data/*"],
      "@services/*": ["src/services/*"],
      "@img/*": ["public/img/*"],
      "@css/*": ["src/css/*"],
      "@assets/*": ["src/assets/*"],
      "@icons/*": ["src/icons /*"]
    }

  }
}
